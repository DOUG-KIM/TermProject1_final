<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.deokwook.termproject1.runner_region2">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:weightSum="1"
        android:id="@+id/linearLayout2">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="시/도"
            android:id="@+id/textView4"
            android:layout_weight="0.33"
            android:textSize="20dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="시/군/구"
            android:id="@+id/textView5"
            android:layout_weight="0.33"
            android:textSize="20dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="동/읍/면"
            android:id="@+id/textView6"
            android:layout_weight="0.34"
            android:textSize="20dp" />
    </LinearLayout>
        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/gridLayout3"
            android:layout_below="@+id/linearLayout2"
            android:layout_alignParentEnd="true">

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button16"
                android:layout_row="0"
                android:layout_column="0"
                android:background="@drawable/address2_1" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button17"
                android:layout_row="0"
                android:layout_column="1"
                android:background="@drawable/address2_2" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button18"
                android:layout_row="0"
                android:layout_column="2"
                android:background="@drawable/address2_3" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button19"
                android:layout_row="1"
                android:layout_column="0"
                android:background="@drawable/address2_4" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button20"
                android:layout_row="1"
                android:layout_column="1"
                android:background="@drawable/address2_5" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button21"
                android:layout_row="1"
                android:layout_column="2"
                android:background="@drawable/address2_6" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button22"
                android:layout_row="2"
                android:layout_column="0"
                android:background="@drawable/address2_7" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button23"
                android:layout_row="2"
                android:layout_column="1"
                android:background="@drawable/address2_8" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button24"
                android:layout_row="2"
                android:layout_column="2"
                android:background="@drawable/address2_9" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button25"
                android:layout_row="3"
                android:layout_column="0"
                android:background="@drawable/address2_10" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button26"
                android:layout_row="3"
                android:layout_column="1"
                android:background="@drawable/address2_11" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button27"
                android:layout_row="3"
                android:layout_column="2"
                android:background="@drawable/address2_12" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button28"
                android:layout_row="4"
                android:layout_column="0"
                android:background="@drawable/address2_13" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button29"
                android:layout_row="4"
                android:layout_column="1"
                android:background="@drawable/address2_14" />

            <Button
                android:layout_width="110dp"
                android:layout_height="58dp"
                android:id="@+id/button30"
                android:layout_row="4"
                android:layout_column="2"
                android:background="@drawable/address2_16" />
        </GridLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/gridLayout3"
            android:layout_alignParentStart="true"
            android:layout_marginTop="58dp">

            <Button
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="닫기"
                android:id="@+id/button31"
                android:layout_weight="1" />

            <Button
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="확인"
                android:id="@+id/button32"
                android:layout_weight="1" />
        </LinearLayout>


</RelativeLayout>
